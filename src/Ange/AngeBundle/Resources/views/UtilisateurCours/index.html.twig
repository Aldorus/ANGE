<h1>UtilisateurCours list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Present</th>
            <th>Prof</th>
            <th>Retard</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('utilisateurcours_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.present }}</td>
            <td>{{ entity.prof }}</td>
            <td>{{ entity.retard }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('utilisateurcours_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('utilisateurcours_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('utilisateurcours_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
